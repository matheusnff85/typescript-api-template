{
  "name": "typescript-api-template",
  "version": "1.0.0",
  "description": "typescript api template",
  "main": "./build/server",
  "scripts": {
    "build": "npx -y tsc",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "prestart": "npm run build && npm run db:reset",
    "start": "node .",
    "predev": "npm run build && npm run db:reset",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts",
    "lint": "eslint ./src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusnff85/typescript-api-template.git"
  },
  "keywords": [
    "typescript-api-template"
  ],
  "author": "Matheus Marinho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matheusnff85/typescript-api-template/issues"
  },
  "homepage": "https://github.com/matheusnff85/typescript-api-template#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.1",
    "@types/sequelize": "4.28.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.7.1",
    "sequelize-cli": "6.3.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mysql2": "2.3.3",
    "sequelize": "6.9.0"
  }
}
